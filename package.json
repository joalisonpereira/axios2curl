{
  "name": "axios2curl",
  "version": "0.0.5",
  "description": "An axios plugin that translates requests into cURL commands",
  "scripts": {
    "dev": "tsx watch src",
    "build": "tsup",
    "test": "vitest",
    "test:coverage": "vitest run --coverage && jest-coverage-badges -o --output './docs'",
    "lint": "npx eslint src --fix",
    "check-types": "tsc --noemit",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "author": {
    "name": "joalisonpereira",
    "email": "joalison.pereira@hotmail.com",
    "url": "https://joalisonpereira.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joalisonpereira/axios2curl.git"
  },
  "bugs": {
    "url": "https://github.com/joalisonpereira/axios2curl/issues"
  },
  "homepage": "https://github.com/joalisonpereira/axios2curl",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "axios",
    "curl",
    "debug"
  ],
  "devDependencies": {
    "@commitlint/config-conventional": "^18.4.2",
    "@types/axios-mock-adapter": "^1.10.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@vitest/coverage-v8": "^0.34.6",
    "axios": "^1.6.2",
    "axios-mock-adapter": "^1.22.0",
    "commitlint": "^18.4.2",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest-coverage-badges": "^1.1.2",
    "lint-staged": "^15.1.0",
    "tsup": "^7.3.0",
    "tsx": "^4.1.3",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "axios": "^1.6.2"
  }
}
